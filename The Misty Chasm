                                              o | - - o |      |
                                             /| |     | |      |
                                            o | |     | |      |
                                            | | |     | |      |
                                        o - - - o - - - o ->  /
                                       /|   | |/|     |/|    /
                                      o | - - o | - - o |   /
                                     /| |   |/| |       v  /
                                    o | | - o | |         /
                                    | | |     | |        /
                              o - - - - o - - - o ->    /
                             /|     | |/|     |/|      /
                            o | - - - o | - - o |     /   The Misty
                           /| |     |/  v       v    /     Caverns
                          o | | - - o  _____________/
                          | | o ->    /
                          | |/       |
                          | o        |
                          |/
                          o
